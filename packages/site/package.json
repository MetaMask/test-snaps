{
  "name": "website",
  "version": "0.3.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/test-snaps.git"
  },
  "private": true,
  "scripts": {
    "setup": "yarn install",
    "build": "node scripts/build.js prod",
    "build:clean": "yarn clean && yarn build",
    "build:dev": "node scripts/build.js dev",
    "clean": "rimraf dist/*",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' '**/*.yml' '**/*.html' --single-quote --ignore-path ../../.gitignore --no-error-on-unmatched-pattern",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:changelog": "yarn auto-changelog validate",
    "publish": "echo N/A"
  },
  "devDependencies": {
    "@metamask/auto-changelog": "^2.5.0",
    "@metamask/eslint-config": "^6.0.0",
    "@metamask/eslint-config-jest": "^6.0.0",
    "@metamask/eslint-config-nodejs": "^6.0.0",
    "@metamask/eslint-config-typescript": "^6.0.0",
    "@metamask/snaps-cli": "^0.10.7",
    "@types/jest": "^26.0.13",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.4.2",
    "mkdirp": "^1.0.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
