{
  "version": "4.6.5",
  "description": "A MetaMask Test Snap that uses the cronjob endowment",
  "proposedName": "MetaMask Cronjob Test Snap",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/test-snaps.git"
  },
  "source": {
    "shasum": "ZWvcv8N8JyDY1ok3x6J9Ym9T5uDcM0esNFVLf5n04KQ=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "iconPath": "images/icon.svg",
        "packageName": "@metamask/test-snap-cronjob",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "endowment:cronjob": {
      "jobs": [
        {
          "expression": "* * * * *",
          "request": {
            "method": "fireCronjob"
          }
        }
      ]
    },
    "snap_dialog": {}
  },
  "manifestVersion": "0.1"
}
